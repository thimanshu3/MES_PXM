<body>

    <%-include('../partials/config')%>

    <div class="app-container app-theme-white body-tabs-shadow fixed-sidebar fixed-header">
        <!-- header/ navbar area -->
        <%-include('../partials/header.ejs')%>
        <!-- header/navbar area end -->

        <div class="app-main">
            <!-- side bar area -->
            <%-include('../partials/sidebar.ejs')%>
            <!-- sidebar area end -->
            <div class="app-main__outer">
                <div class="app-main__inner">
                    <div class="app-page-title">
                        <div class="page-title-wrapper">
                            <div class="page-title-heading">
                                <div class="page-title-icon">
                                    <i class="fas fa-home icon-gradient bg-mean-fruit">
                                    </i>
                                </div>
                                <div>Track student progress
                                    <div class="page-title-subheading">
                                        <div class="d-flex justify-content-center ">
                                            track student progress by each unit and topic completions.
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="card col-12">
                            <div class="card-body">
                                <%Object.keys(CourseDetails).forEach(key => {%>
                                <div class="row mb-3">
                                    <a class="col-5" style="width: 100%;" data-toggle="collapse" href="#id-<%=key%>"
                                        class="collapsed" aria-expanded="false" style="text-decoration: none;">
                                        <% if (CourseDetails[key].completedAt != null) { %>
                                        <h5 class="card-title text-primary">
                                            <svg fill="#1F8354" class="_ufjrdd" viewBox="0 0 48 48" role="img"
                                                aria-labelledby="Completed996db21a-9ba9-468e-b8bd-20d625b1a406 Completed996db21a-9ba9-468e-b8bd-20d625b1a406Desc"
                                                xmlns="http://www.w3.org/2000/svg"
                                                style="fill: rgb(54, 59, 66); height: 22px; width: 22px; margin-right: 12px;">
                                                <title id="Completed996db21a-9ba9-468e-b8bd-20d625b1a406">
                                                    Completed</title>
                                                <path
                                                    d="M1 24C1 11.318375 11.318375 1 24 1s23 10.318375 23 23-10.318375 23-23 23S1 36.681625 1 24zm20.980957 4.2558594l-7.7418213-7.0596924L12 23.5592041l9.980957 9.6016846 15.2832032-16.4852295L34.9130859 14 21.980957 28.2558594z"
                                                    fill="#1F8354" role="presentation">
                                                </path>
                                            </svg>

                                            UNIT - <%=CourseDetails[key].unitNumber%></h5>
                                        <%} else{ %>
                                        <h5 class="card-title text-primary">
                                            UNIT - <%=CourseDetails[key].unitNumber%></h5>
                                        <%}%>
                                                        </a>
                                                        <div class="col-7">
                                                            <% if (CourseDetails[key].completedAt != null) { %>
                                        <span class=" text-dark" style="float: right;">
                                            <b><%= formatDateMoment(CourseDetails[key].completedAt)%>
                                            </b></span>
                                        <%} else{ %>
                                        <span class="text-danger" style="float: right;">
                                            <b>Incomplete</b>
                                        </span>
                                        <%}%>
                                                        </div>
                                                    </div>
                                                    <div class="collapse" id="id-<%=key%>" style="">
                                        <div class="scroll-area-md">
                                            <div class="scrollbar-container ps--active-y ps">
                                                <ul class="list-group list-group-flush border">
                                                    <%CourseDetails[key].topics.forEach(topic => {%>
                                                    <li class="list-group-item">
                                                        <a class="nostyle">
                                                            <div class="_errb565 lms-WeekSingleItemDisplay">
                                                                <div class="_1b2lknu2">
                                                                    <div class="_y1d9czk d-flex">
                                                                        <% if(topic.completedAt != null){ %>
                                                                        <svg fill="#1F8354" class="_ufjrdd"
                                                                            viewBox="0 0 48 48" role="img"
                                                                            aria-labelledby="Completed996db21a-9ba9-468e-b8bd-20d625b1a406 Completed996db21a-9ba9-468e-b8bd-20d625b1a406Desc"
                                                                            xmlns="http://www.w3.org/2000/svg"
                                                                            style="fill: rgb(54, 59, 66); height: 22px; width: 22px; margin-right: 12px;">
                                                                            <title
                                                                                id="Completed996db21a-9ba9-468e-b8bd-20d625b1a406">
                                                                                Completed</title>
                                                                            <path
                                                                                d="M1 24C1 11.318375 11.318375 1 24 1s23 10.318375 23 23-10.318375 23-23 23S1 36.681625 1 24zm20.980957 4.2558594l-7.7418213-7.0596924L12 23.5592041l9.980957 9.6016846 15.2832032-16.4852295L34.9130859 14 21.980957 28.2558594z"
                                                                                fill="#1F8354" role="presentation">
                                                                            </path>
                                                                        </svg>
                                                                        <%} else { %>
                                                                        <svg fill="#1F8354" class="_ufjrdd"
                                                                            viewBox="0 0 48 48" role="img"
                                                                            aria-labelledby="Completed996db21a-9ba9-468e-b8bd-20d625b1a406 Completed996db21a-9ba9-468e-b8bd-20d625b1a406Desc"
                                                                            xmlns="http://www.w3.org/2000/svg"
                                                                            style="fill: rgb(54, 59, 66); height: 22px; width: 22px; margin-right: 12px;">
                                                                            <title
                                                                                id="Completed996db21a-9ba9-468e-b8bd-20d625b1a406">
                                                                                Completed</title>
                                                                            <path
                                                                                d="M1 24C1 11.318375 11.318375 1 24 1s23 10.318375 23 23-10.318375 23-23 23S1 36.681625 1 24zm20.980957 4.2558594l-7.7418213-7.0596924L12 23.5592041l9.980957 9.6016846 15.2832032-16.4852295L34.9130859 14 21.980957 28.2558594z"
                                                                                fill="#808080" role="presentation">
                                                                            </path>
                                                                        </svg>
                                                                        <% } %>
                                                                        <div class="_y1d9czk">
                                                                            <div
                                                                                class="lms-WeekItemName headline-1-text">
                                                                                <strong>Reading: </strong>
                                                                                <%=topic.name%>
                                                                            </div>

                                                                            <div
                                                                                class="_1qfi0x77 lms-WeekItemAnnotations">
                                                                                <% if(topic.completedAt != null){ %>
                                                                                <span
                                                                                    class="lms-EffortText caption-text text-hint">
                                                                                    <%= formatDateMoment(topic.completedAt)%>
                                                                                </span>
                                                                                <%} else { %>
                                                                                <%= topic.duration %> min
                                                                                <%}%>
                    
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </a>
                                                                    </li>
                                                                    <%})%>
                                                </ul>
                                            </div>
                                        </div>
                                </div>
                                <%})%>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="/js/main.js"></script>
    <script src="/js/jquery.min.js"></script>
    <script src="/js/iziToast.min.js"></script>
    <script src="/js/flashMessage.js"></script>
</body>

</html>
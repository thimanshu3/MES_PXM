    .lms-advantage .features .feature .image {
        width: 17rem;
        height: 10rem;
        background-repeat: no-repeat;
        background-size: cover;
        background-position: center;
        margin-right: 1.563rem;
        margin-bottom: 1.563rem;
        display: none;
    }

    .lms-advantage .features .feature .title {
        font-weight: 600;
        font-size: 1.125rem;
        color: #000;
        line-height: 2rem;
        margin-top: 0.625rem;
        margin-bottom: 0.625rem;
        margin-left: 1.125rem;
        width: 12.5rem;
    }

    .lms-advantage .features .feature .description {
        font-size: 0.875rem;
        line-height: 1.5rem;
        max-width: 20rem;
        margin: 0.9375rem 1.25rem 1.5625rem 1.563rem;
    }

    .lms-advantage .features .feature .icon-image {
        box-shadow: 10px 10px 25px 0 rgba(0, 0, 0, 0.1);
        background: #fff;
        width: 3.875rem;
        height: 3.875rem;
        border-radius: 50%;
        display: -webkit-box;
        display: flex;
        -webkit-box-pack: center;
        justify-content: center;
        -webkit-box-align: center;
        align-items: center;
    }

    .lms-advantage .features .feature .icon-image img {
        height: auto;
    }

    @media screen and (min-width: 48rem) {
        .lms-advantage .features .feature {
            -webkit-box-orient: horizontal;
            -webkit-box-direction: normal;
            flex-direction: row;
            max-width: none;
            -webkit-box-pack: justify;
            justify-content: space-between;
            margin: 1.563rem;
        }

        .lms-advantage .features .feature .feature-header {
            margin-left: -4rem;
            padding-top: 1rem;
            -webkit-box-align: normal;
            align-items: normal;
        }

        .lms-advantage .features .feature .image {
            -webkit-box-flex: 1;
            flex: 1 0 13.125rem;
            height: 13.125rem;
            width: 13.125rem;
            margin-right: 2rem;
            margin-bottom: 0;
            display: block;
        }

        .lms-advantage .features .feature .title {
            font-size: 1.125rem;
            line-height: 2rem;
            margin-top: 0;
            margin-left: 1.5rem;
            width: 15.63rem;
        }

        .lms-advantage .features .feature .description {
            font-size: 0.875rem;
            line-height: 1.5rem;
            width: 28rem;
        }
    }

    @media screen and (min-width: 62rem) {
        .lms-advantage .features .feature .title {
            width: 25rem;
        }

        .lms-advantage .features .feature .description {
            max-width: none;
            width: 31.25rem;
        }
    }

    @media screen and (min-width: 73.75rem) {
        .lms-advantage .features .feature {
            width: 33.5rem;
        }

        .lms-advantage .features .feature:nth-child(2n) {
            margin-left: 2rem;
        }

        .lms-advantage .features .feature .title {
            width: 15.63rem;
        }

        .lms-advantage .features .feature .description {
            max-width: none;
            width: 18.75rem;
        }
    }
</style>


<style>
    @media (max-width: 767.98px) {
        .app-container .page-top-wrapper::after {
            display: none !important;
        }

        .app-container .page-top-wrapper .page-hero {
            min-height: 100px;
            height: auto;
            padding: 6.75rem 0 1.5rem;
        }

        .app-container .page-top-wrapper .page-hero .hero-p {
            margin-top: 1rem;
            line-height: 1.6rem;
        }

        .app-container .page-top-wrapper .page-hero .hero-h3 {
            display: none;
        }

        .app-container .page-top-wrapper .page-hero .page-hero-inner {
            padding: 1.5rem;
        }

        .icon-wrapper {
            margin-top: 0.75rem !important;
        }

        .page-top-wrapper-alt2 .page-hero {
            padding: 1.5rem 0 !important;
        }

        .page-top-wrapper-alt2 .page-hero .page-hero-inner {
            padding-top: 0 !important;
            padding-bottom: 0 !important;
        }

        .page-top-wrapper-alt2 .page-hero .page-hero-inner .hero-h3 {
            display: block !important;
        }

        .page-top-wrapper-alt2 .page-hero .page-hero-inner .hero-p.mb-5 {
            margin-bottom: 1.5rem !important;
        }
    }

    .icon-wrapper {
        display: flex;
        align-content: center;
        align-items: center;
    }

    .icon-wrapper {
        width: 54px;
        height: 54px;
        margin: 5px auto 0.66667rem;
        position: relative;
        overflow: hidden;
    }

    .icon-wrapper .icon-wrapper-bg {
        position: absolute;
        height: 100%;
        width: 100%;
        z-index: 3;
        opacity: .2;
    }

    .icon-wrapper i {
        margin: 0 auto;
        font-size: 1.7rem;
        position: relative;
        z-index: 5;
    }

    .icon-wrapper i:before {
        margin-top: -3px;
    }

    [class^="lnr-"] {
        font-family: 'Linearicons-Free';
        speak: none;
        font-style: normal;
        font-weight: normal;
        font-variant: normal;
        text-transform: none;
        line-height: 1;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }

    .lnr-store:before {
        content: "\E82D";
    }

    .page-top-wrapper {
        background-attachment: fixed;
        background-position: center;
        background-repeat: no-repeat;
        background-size: cover;
        position: relative;
        background-color: #2a2b2f;
        background-image: url(https://wallpapercave.com/wp/wp3404265.jpg);
        border-bottom: #dee2e6 solid 1px;
    }

    .page-top-wrapper::before {
        content: '';
        position: absolute;
        left: 0;
        top: 0;
        z-index: 7;
        width: 100%;
        height: 100%;
        background: rgba(42, 43, 47, 0.3);
    }

    .page-top-wrapper-alt .page-hero {
        height: auto;
    }

    .page-top-wrapper-alt2 .page-hero {
        min-height: 100px;
    }

    .page-hero {
        position: relative;
        min-height: 500px;
    }

    .page-hero .hero-p,
    .page-hero .hero-h3 {
        color: #fff;
        text-align: center;
        max-width: 740px;
        margin: 0 auto;
        display: block;
    }

    .page-hero .hero-p {
        font-size: 1.25rem;
        color: #adb5bd;
    }

    .page-hero .hero-h3 {
        font-size: 2.3rem;
        font-weight: 500;
        font-weight: normal;
        margin: 2rem auto 1.2rem;
    }

    .page-hero .page-hero-inner {
        margin: 0 auto;
        position: relative;
        z-index: 7;
        padding: 50px 0 0;
        width: 100%;
    }

    .page-hero-bottom .page-hero-inner {
        padding: 3rem 0;
        text-align: center;
    }

    h3 {
        font-family: "Bariol", sans-serif;
    }

    .btn-big {
        font-family: "Rubik", sans-serif;
        font-size: 1rem;
        font-weight: 400;
    }

    .buy-buttons .btn-success {
        padding: 1.3rem 2rem;
        text-transform: uppercase;
        font-size: 1.1rem;
    }

    .buy-buttons .btn-success span {
        opacity: .8;
        padding-right: 1rem;
    }

    @media (max-width: 991.98px) {
        .page-hero .hero-h3 {
            font-size: 1.4rem;
        }

        .page-hero .hero-p {
            font-size: .9rem;
        }
    }

    /*! CSS Used fontfaces */
    @font-face {
        font-family: 'Linearicons-Free';
        src: url(https://architectui.com/static/media/Linearicons-Free.b9b7f23c.eot);
        src: url(https://architectui.com/static/media/Linearicons-Free.b9b7f23c.eot) format("embedded-opentype"), url(https://architectui.com/static/media/Linearicons-Free.03e91f12.woff2) format("woff2"), url(https://architectui.com/static/media/Linearicons-Free.65060723.woff) format("woff"), url(https://architectui.com/static/media/Linearicons-Free.2f3e9f80.ttf) format("truetype"), url(https://architectui.com/static/media/Linearicons-Free.71ad32ce.svg) format("svg");
        font-weight: normal;
        font-style: normal;
    }

    @font-face {
        font-family: 'Bariol';
        font-style: normal;
        font-weight: 500;
        src: local("Bariol"), local("Bariol"), url(https://architectui.com/static/media/BariolRegular.565752f3.woff) format("woff");
    }

    @font-face {
        font-family: 'Rubik';
        font-style: normal;
        font-weight: 400;
        src: local("Rubik"), local("Rubik"), url(https://architectui.com/static/media/rubik-400-normal.cebc4ea4.woff2) format("woff2");
    }

    img {
        border-style: none;
    }


    .contain {
        max-width: 73.75rem;
        margin-left: auto;
        margin-right: auto;
        padding-top: 48px;
        padding-bottom: 48px;
        padding-left: 12px;
        padding-right: 12px;
    }

    .contain::after {
        clear: both;
        content: "";
        display: block;
    }

    @media screen and (min-width: 48rem) {
        .contain {
            padding-top: 96px;
            padding-bottom: 96px;
        }
    }

    @media screen and (min-width: 30rem) {
